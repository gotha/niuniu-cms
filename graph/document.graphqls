
type Document {
  id: String!
  title: String!
  body: String!
  createdAt: String!
  updatedAt: String!
  tags: [Tag!]!
}

type Documents {
  documents: [Document!]!
  count: Int!
}

input NewDocument {
  title: String!
  body: String!
  tags: [String!]
}

input UpdateDocument {
  title: String
  body:  String
  tags: [String]
}

